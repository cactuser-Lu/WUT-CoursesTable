/* pages/fortest/fortest.wxss */
page{
  font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
  background-color: rgb(248, 248, 248);
    height: 100%;
}
.avatar-wrapper {
  background: #1b82d1;
  padding: 25px 0;
} .avatar-wrapper .avatar {
  margin: 0 auto;
  text-align: center;
} .avatar-wrapper .avatar image {
  width: 100px;
  height: 100px;
} .avatar-wrapper .text {
  text-align: center;
  color: #fff;
} .weui-cell__hd image {
  margin-right: 5px;
  vertical-align: middle;
  width: 20px;
  height: 20px;
} .weui-cells::after{
  content: none;
  border-bottom: none;
} .weui-cells::before{
  border-top: none;
} 

#feedback{ 
height: auto;
width: auto;
font-size: 38rpx;
text-align: left;
line-height: 2.0;
background-color: #FFFFFF;
}
.shadow {
box-shadow: 0 0rpx 8rpx rgba(0, 0, 0, 0.2);
}